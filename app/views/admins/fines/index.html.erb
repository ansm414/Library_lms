<h1> Fines </h1>



 <table class="table table-striped">
    <thead>
      <tr>
      <th> ID </th>
      <th> Book Name </th>
      <th> User Name </th>


        <th>Issued Book id</th>
        <th>Fine</th>
    


       



        




      </tr>
    </thead>
    <tbody>


        <% @fines.each do |fine| %>
        <tr>
        <td>  <%=  fine.id %></td>
        <td>  <%=  fine.issued_book.book.name %></td>
        <td>  <%=  fine.issued_book.user.name %></td>


        <td>  <%=  fine.issued_book_id %></td>
        <td>  <%=  fine.fine %></td>



   
      


    

      </tr>
  

    <%end%>



     

    </tbody>
  </table>




